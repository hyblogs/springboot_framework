<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <context id="Mysql" targetRuntime="MyBatis3Simple" defaultModelType="flat">
    <property name="javaFileEncoding" value="UTF-8"/>
    
    <plugin type="tk.mybatis.mapper.generator.MapperPlugin">
      	<property name="mappers" value="tk.mybatis.mapper.common.Mapper,tk.mybatis.mapper.common.MySqlMapper"/>
      <property name="caseSensitive" value="true"/>
    </plugin>

    <plugin type="com.generator.SelectByGenSqlPlugin"/>

    <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>

    <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                    connectionURL="jdbc:mysql://123.56.18.97:3306/framework"
                    userId="root"
                    password="hy865571.">
    </jdbcConnection>

    <javaModelGenerator targetPackage="com.framework.entity.mapping" targetProject="src/main/java"/>

    <sqlMapGenerator targetPackage="com.framework.mapper.mapper" targetProject="src/main/java"/>

    <javaClientGenerator targetPackage="com.framework.mapper" targetProject="src/main/java" type="XMLMAPPER"/>

    <!--系统架构数据库表-->
    <table tableName="site_user" schema="framework"><generatedKey column="id" sqlStatement="Mysql" identity="true"/> </table>
